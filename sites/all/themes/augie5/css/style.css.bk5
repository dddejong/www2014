/* AUGUSTANA WEB SITE STYLES
--------------------------------------------------------------------------- */	

/* color palette:

light blue: #ceeoee;
medium blue: #47638c
dark blue: #002866
gold: #ffd22b
light gold: #fef2b1
barely visible gray: #f4f4f4
lighter gray: #e7e7e7
light gray: #cccccc
*/


body {
	font-family:"Helvetica Neue", Helvetica, arial, sans-serif;
	font-size:12px;
	line-height:1.5em;
	color:#323232;
	margin:0;
	padding:0;
	background:url(../images/bckgrnd.gif) repeat-x left top #cee0ee;
	}

/* STRUCTURE/LAYOUT
--------------------------------------------------------------------------- */	

#wrapper { /*gives the left-hand shadow*/
	width:881px;
	padding:135px 0 0 7px;
	background:url(../images/shad-left.png) repeat-y;
	margin:0 auto;
	}
	
#wrapper #wrapper-shade { /*gives the right-hand shadow*/
	width:874px;
	background:url(../images/shad-right.png) repeat-y right top;
	padding:0 7px 0 0;
	}
	
#wrapper #content {
	width:854px;
	background:#fff;
	padding:13px 0 20px 20px;
	}
	
#wrapper #content #main {
	float:right;
	width:600px;
	margin-bottom:1em;
	}
		
#wrapper #content #main-admin,
#wrapper #content #main-single, {
	width:834px;
	margin-bottom:1em;
	padding: 20px 0 0 0;
	}	
	
#wrapper #content #main-news {
	float:left;
	margin: 0 20px 0 0;
	width:380px;
	}	

#wrapper #content #main-news h2 {
	margin: 0  0 10px 0;
	}	

#wrapper #content #main-news ul.tabs {
	margin-right: -230px;
	}

.front #wrapper #content #main { /*home page only*/
	float:left;
	width:600px;
	}
	
	
/* STANDARD ELEMENTS
--------------------------------------------------------------------------- */	
	
img {
	border:none;
	}

a, a:link, a:active {
	color: #47638c;
	text-decoration:underline;
	}
	
a:visited {
	color: #323232;
	}
	
a:hover {
	color:#00274d;
	}
	
h1 { font-size:2em; line-height:1.6em; margin:.3em 0; padding:0; }
h2 { font-size:1.7em; line-height:1.2em; margin:.2em 0 .8em 0; padding:0; }
h3 { font-size:1.3em; line-height:1.2em; margin:.1em 0 .2em 0; padding:0; }
h4 { font-size:1.2em; line-height:1em; margin:.1em 0; padding:0; }
h5 { font-size:1.1em; line-height:1em; margin:.1em 0; padding:0; }
h6 { font-size:1em; line-height:1em; margin:.1em 0; padding:0; }

h3.title,
h2.title {
	margin:0 0 2.0em 0;
	padding:0;
	font-size:10px;
	line-height:1em;
	font-weight:normal;
	letter-spacing:.2em;
	text-transform:uppercase;
	}


p {
	margin: 0 0 12px 0;
	}



/* HEADER
--------------------------------------------------------------------------- */	

#header {
	position:relative;
	margin:0 auto;
	width:888px;
	}
	
h1#logo {
   background:transparent url(../images/logo.gif) no-repeat scroll;
   left top;
   height:135px;
   left:-50px;
   margin:0pt;
   padding:0pt;
   position:absolute;
   text-indent:-9999em;
   width:874px;
   }

h1#logo a {
	display:block;
	text-indent:-9999px;
    background:transparent url(../images/logo.gif) no-repeat scroll;
    width: 380px;
    height: 100px;
	}

form#site-search {
	position:absolute;
	top:50px;
	right:8px;
	}
	
	form#site-search input#keywords {
		width:182px;
		height:24px;
		border:none;
		background:url(../images/bck-search-input.gif) no-repeat left top;
		float:left;
		display:inline;
		}
		
	form#site-search input#search {
		width:73px;
		height:24px;
		border:none;
		text-indent:-9999em;
		background:url(../images/bttn-search.gif) no-repeat left top;
		float:left;
		display:inline;
		}
		
form#google-cse-searchbox-form {
	position:absolute;
	top:50px;
	right:8px;
	}
	
	form#google-cse-searchbox-form input#edit-query {
		width:182px;
		height:24px;
		border:none;
		background:url(../images/bck-search-input.gif) no-repeat left top;
		float:left;
		display:inline;
		padding: 3px 0 0 0;
		}
		
	form#google-cse-searchbox-form input#edit-sa {
		width:73px;
		height:24px;
		border:none;
		text-indent:-9999em;
		background:url(../images/bttn-search.gif) no-repeat left top;
		float:left;
		display:inline;
		padding-top:40px; overflow:hidden;
		}

form#google-cse-searchbox-form div {
	margin: 0;
	padding: 0;
	line-height: 0;
	}
	
form#google-cse-searchbox-form input.form-text {
  border: none;
  padding: 0;
	}

form#google-cse-results-searchbox-form {
	margin: 0 0 0 0;
	}

form#google-cse-results-searchbox-form input#edit-query {
  border: 1px solid #ccc;
  padding: 0;
  height:20px;
	}

ul#tools {
	list-style:none;
	margin:0;
	padding:0;
	position:absolute;
	top:50px;
	right:270px;
	}
	
	ul#tools li {
		display:inline;
		color:#fff;
		font-size:11px;
		padding:0 10px 0 0;
		margin:0 auto;
		}
		
		ul#tools li.li-cap{
			border:none;
			}
			
		ul#tools li a {
			color:#fff;
			text-decoration: underline;
			}
		
			
/* PRIMARY (TOP) NAVIGATION
--------------------------------------------------------------------------- */	
		
ul#nav {
	list-style:none;
	margin:0 auto;
	padding:0 10px;
	position:absolute;
	width:854px;
	height:39px;
	top:93px;
	left:7px;
	text-align:center;
	background:#47638c;
	border-top:3px solid #e1ce5a;
	}
	
	ul#nav li {
		display:inline;
		color:#fff;
		font-size:14px;
		line-height:40px;
		padding:0 10px;
		margin:0 auto;
		border-right:1px solid #fff;
		}
		
		ul#nav li.last {
			border:none;
			}
			
		ul#nav li a {
			color:#fff;
			text-decoration: none;
			}
			
		ul#nav li a:hover,
		ul#nav li a.here {
			text-decoration: underline;
			}
			

