/* Section/Content Page > Section Navigation
--------------------------------------------------------------------------- */	

#wrapper #content #sidebar {
	float:left;
	width:220px;
	padding: 0 20px 0 0;
	}

#wrapper #content #sidebar h2 {
	font-size: 15px;
	text-transform: uppercase;
	}
	
#wrapper #content #sidebar a {
	text-decoration: none;
	}
	
	#wrapper #content #sidebar ul { /*first level*/
		margin:0 0 8px 0;
		padding:0 0 30px 0;
		list-style:none;
		background-image: none;
		list-style-image: none;
		list-style-type:none;
		}
		
	#wrapper #content #sidebar ul.side-nav {
		}
		
		#wrapper #content #sidebar ul.side-nav li {
			background-image: none;
			border-bottom:1px solid #ececec;
			font-size:14px;
			font-weight:bold;
			padding:0;
			margin:0 0 10px 0;
			list-style:none;
			list-style-image: none;
			list-style-type:none;
			line-height:1.2em;
			}
			
			#wrapper #content #sidebar ul.side-nav li a {
				padding: 5px;
				/*background:#e5eefa;*/
				display:block;
				color:#002866;
				}
				
			#wrapper #content #sidebar ul.side-nav li a:hover {
				background:#002866;
				color:#fff;
				}
				
			#wrapper #content #sidebar ul.side-nav li.here {
				border: none;
				}
				
			#wrapper #content #sidebar ul.side-nav li.here a {
				padding:5px;
				background:#002866;
				color:#fff;
				}
				
			#wrapper #content #sidebar ul.side-nav li.here ul {  /*second level*/
				margin:0 0 0 8px;
				padding:0;
				}
				
				#wrapper #content #sidebar ul.side-nav li.here ul li {
					font-size:12px;
					text-align:left;
					padding:5px 0 0 0;
					margin:0 0 0 20px;
					}
					
					#wrapper #content #sidebar ul.side-nav li.here ul li a {
						background:#fff;
						color:#002866;
						}
						
					#wrapper #content #sidebar ul.side-nav li.here ul li a:hover {
						background:#002866;
						color:#fff;
						}
						
						
					#wrapper #content #sidebar ul.side-nav li.here ul li.here.highlight a {
						background:#FFFFFF;
						color:#000;
						border-bottom:1px solid #ececec;
						}
					
					#wrapper #content #sidebar ul.side-nav li.here ul li.here a {
						background:#FFFFFF;
						color:#000;
						border-bottom:1px solid #ececec;
						}
					
					#wrapper #content #sidebar ul.side-nav li.here ul li.li-cap-bttm a {
						background:#fff;
						color:#002866;
						border:none;
						}
						
					#wrapper #content #sidebar ul.side-nav li.here ul li.li-cap-bttm a:hover {
						background:#002866;
						color:#fff;
						}
						
						#wrapper #content #sidebar ul.side-nav li.here ul li.here ul { /*third level*/
							padding:0 0 0 30px;
							margin:0;
							}
							
							#wrapper #content #sidebar ul.side-nav li.here ul li.here ul li {
								font-size:12px;
								font-weight:normal;
								padding:5px 0;
								margin:5px 0;
								}
								
								#wrapper #content #sidebar ul.side-nav li.here ul li.here ul li a {
									/*background:#fff1bd;*/
									background:#fff;
									border:none;
									color:#40381d;
									display:inline;
									padding:3px;
									}
									
								#wrapper #content #sidebar ul.side-nav li.here ul li.here ul li a:hover {
									background:#002866;
									color:#fff;
									}
									
								#wrapper #content #sidebar ul.side-nav li.here ul li.here ul li.here {
									border-bottom:1px solid #ececec;
									}
									
									#wrapper #content #sidebar ul.side-nav li.here ul li.here ul li.here a {
										/*background:#002866;*/
										color:#000;
										font-weight: bold;
										}
									#wrapper #content #sidebar ul.side-nav li.here ul li.here ul li.here a:hover {
									    background:#fff;
										color:#000;
										font-weight: bold;
										}
					
					#wrapper #content #sidebar ul.side-nav ul ul li a {
						border-bottom: 0;
						background: pink;
						}
						
						
				
		#wrapper #content #sidebar ul.secondary-nav li { /*choose your path nav*/
			font-size:12px;
			line-height:1.7em;
			padding:5px 0;
