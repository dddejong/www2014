/* PRIMARY (TOP) NAVIGATION
--------------------------------------------------------------------------- */	
		
ul#nav {
	list-style:none;
	margin:0 auto;
	padding:0 10px;
	position:absolute;
	width:854px;
	height:39px;
	top:93px;
	left:7px;
	text-align:center;
	background:#47638c;
	border-top:3px solid #e1ce5a;
	}
	
	ul#nav li {
		display:inline;
		color:#fff;
		font-size:14px;
		line-height:40px;
		padding:0 10px;
		margin:0 auto;
		border-right:1px solid #fff;
		}
		
		ul#nav li.last {
			border:none;
			}
			
		ul#nav li a {
			color:#fff;
			text-decoration: none;
			}
			
		ul#nav li a:hover,
		ul#nav li a.active-trail {
			text-decoration: underline;
			}
			

