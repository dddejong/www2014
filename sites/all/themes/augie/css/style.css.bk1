/* Section/Content Page > Section Navigation
--------------------------------------------------------------------------- */	

#sidebar {
	float:left;
	width:220px;
	padding: 0 20px 0 0;
	}

#sidebar h2 {
	font-size: 15px;
	text-transform: uppercase;
	}
	
#sidebar a {
	text-decoration: none;
	}
	
	#sidebar ul { /*first level*/
		margin:0 0 8px 0;
		padding:0 0 30px 0;
		list-style:none;
		background-image: none;
		list-style-image: none;
		list-style-type:none;
		}
	
	#sidebar ul.menu li.expanded ul li.active-trail {
		border: none;
		}
	
	#sidebar ul.menu {
		}
		
		#sidebar ul.menu li {
			background-image: none;
			border-bottom:1px solid #ececec;
			font-size:14px;
			font-weight:bold;
			padding:0;
			margin:0 0 10px 0;
			list-style:none;
			list-style-image: none;
			list-style-type:none;
			line-height:1.2em;
			}
			
			#sidebar ul.menu li a {
				padding: 5px;
				/*background:#e5eefa;*/
				display:block;
				color:#002866;
				}
				
			#sidebar ul.menu li a:hover {
				background:#002866;
				color:#fff;
				}
				
			#sidebar ul.menu li.active-trail {
				border: none;
				}
				
			#sidebar ul.menu li.active-trail a {
				padding:5px;
				background:#002866;
				color:#fff;
				}
				
			#sidebar ul.menu li.active-trail ul {  /*second level*/
				margin:0 0 0 8px;
				padding:0;
				}
				
				#sidebar ul.menu li.active-trail ul li {
					font-size:12px;
					text-align:left;
					padding:5px 0 0 0;
					margin:0 0 0 20px;
					}
					
					#sidebar ul.menu li.active-trail ul li a {
						background:#fff;
						color:#002866;
						}
						
					#sidebar ul.menu li.active-trail ul li a:hover {
						background:#002866;
						color:#fff;
						}
						
						
					#sidebar ul.menu li.active-trail ul li.active-trail.highlight a {
						background:#FFFFFF;
						color:#000;
						/* border-bottom:1px solid #ececec; */
						}
					
					#sidebar ul.menu li.active-trail ul li.active-trail a {
						background:#FFFFFF;
						color:#000;
						border-bottom:1px solid #ececec;
						}
					
					#sidebar ul.menu li.active-trail ul li.li-cap-bttm a {
						background:#fff;
						color:#002866;
						border:none;
						}
						
					#sidebar ul.menu li.active-trail ul li.li-cap-bttm a:hover {
						background:#002866;
						color:#fff;
						}
						
						#sidebar ul.menu li.active-trail ul li.active-trail ul { /*third level*/
							padding:0 0 0 30px;
							margin:0;
							}
							
							#sidebar ul.menu li.active-trail ul li.active-trail ul li {
								font-size:12px;
								font-weight:normal;
								padding:5px 0;
								margin:5px 0;
								line-height: 1.5em;
								}
								
								#sidebar ul.menu li.active-trail ul li.active-trail ul li a {
									/*background:#fff1bd;*/
