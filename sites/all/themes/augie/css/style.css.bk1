/* AUGUSTANA WEB SITE STYLES
--------------------------------------------------------------------------- */	

body {
	font-family:"Helvetica Neue", Helvetica, arial, sans-serif;
	font-size:12px;
	line-height:1.5em;
	color:#323232;
	margin:0;
	padding:0;
	background:url(../images/bckgrnd.gif) repeat-x left top #cee0ee;
	}

/* STRUCTURE/LAYOUT
--------------------------------------------------------------------------- */	

#wrapper { /*gives the left-hand shadow*/
	width:881px;
	padding:135px 0 0 7px;
	background:url(../images/shad-left.png) repeat-y;
	margin:0 auto;
	}
	
#wrapper #wrapper-shade { /*gives the right-hand shadow*/
	width:874px;
	background:url(../images/shad-right.png) repeat-y right top;
	padding:0 7px 0 0;
	}
	
#wrapper #content {
	width:854px;
	background:#fff;
	padding:13px 0 20px 20px;
	}
	
#main {
	float:right;
	width:600px;
	margin-bottom:1em;
	
/*	padding-right: 30px;*/
	}
		
#main-admin,
#main-single, {
	width:834px;
	margin-bottom:1em;
	padding: 20px 0 0 0;
	}	
	
#main-news {
	float:left;
	margin: 0 20px 0 0;
	width:380px;
	}	

#main-news h2 {
	margin: 0  0 10px 0;
	}	

#main-news ul.tabs {
	margin-right: -230px;
	}

.front #main { /*home page only*/
	float:left;
	width:600px;
	}
	
	
/* STANDARD ELEMENTS
--------------------------------------------------------------------------- */	
	
img {
	border:none;
	}

a, a:link, a:active {
	color: #47638c;
	text-decoration:underline;
	}
	
a:visited {
	color: #323232;
	}
	
a:hover {
	color:#00274d;
	}
	
h1 { font-size:2em; line-height:1.6em; margin:.3em 0; padding:0; }
h2 { font-size:1.7em; line-height:1.2em; margin:.2em 0 .8em 0; padding:0; }
h3 { font-size:1.3em; line-height:1.2em; margin:.1em 0 .2em 0; padding:0; }
h4 { font-size:1.2em; line-height:1em; margin:.1em 0; padding:0; }
h5 { font-size:1.1em; line-height:1em; margin:.1em 0; padding:0; }
h6 { font-size:1em; line-height:1em; margin:.1em 0; padding:0; }

h3.title,
h2.title,
.block h2,
#sidebar .block-menu h2,
#sidebar .block-menu_block h2 {
	margin:0 0 2.0em 0;
	padding:0;
	font-size:10px;
	line-height:1em;
	font-weight:normal;
	letter-spacing:.2em;
	text-transform:uppercase;
	}


p {
	margin: 0 0 12px 0;
	}

.item-list ul li {
	margin: 0 0 0.25em 1.5em;
}

